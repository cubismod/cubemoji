{
  "name": "cubemoji",
  "author": "cubis",
  "version": "3.3.3",
  "description": "A Discord emoji bot",
  "main": "Main.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --es-module-specifier-resolution=node build/Main.js",
    "inspect": "node --inspect --es-module-specifier-resolution=node build/Main.js",
    "profile": "clinic doctor -- node --es-module-specifier-resolution=node build/Main.js",
    "test": "tsc && export CM_TEST=true && node --es-module-specifier-resolution=node build/tests/Tests.js",
    "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node --es-module-specifier-resolution=node --trace-warnings --inspect build/Main.js\"",
    "clean": "rm -rf build/",
    "lint": "eslint",
    "audit": "yarn install --audit"
  },
  "eslintPreCommitBaseCommand": "eslint --fix --max-warnings=0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/cubismod/cubemoji.git"
  },
  "keywords": [
    "discord",
    "emoji",
    "bot"
  ],
  "bugs": {
    "url": "https://gitlab.com/cubismod/cubemoji/issues"
  },
  "homepage": "https://gitlab.com/cubismod/cubemoji#readme",
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/koa": "^1.0.8",
    "@discordx/pagination": "^2.2.2",
    "@discordx/utilities": "4.3.3",
    "@keyv/sqlite": "^3.5.2",
    "@koa/router": "^10.1.1",
    "@types/koa": "^2.13.4",
    "better-sqlite3": "^7.5.3",
    "chokidar": "^3.5.3",
    "color-thief-node": "^1.0.4",
    "dayjs": "^1.11.3",
    "discord.js": "^13.6.0",
    "discordx": "9.7.3",
    "dotenv": "^16.0.1",
    "file-type": "^17.1.1",
    "fuse.js": "^6.6.1",
    "gm": "^1.23.1",
    "got": "^12.0.4",
    "image-downloader": "^4.3.0",
    "isbinaryfile": "^5.0.0",
    "js-quantities": "^1.7.6",
    "keyv": "^4.2.2",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-compress": "^5.1.0",
    "koa-send": "^5.0.1",
    "micromatch": "^4.0.4",
    "node-emoji": "^1.11.0",
    "node-object-hash": "^2.3.10",
    "pandemonium": "^2.3.0",
    "pify": "^6.0.0",
    "probe-image-size": "^7.2.3",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rgb-hex": "^4.0.0",
    "simple-git": "^3.7.1",
    "tslib": "^2.4.0",
    "tsyringe": "^4.6.0",
    "twemoji-parser": "^14.0.0",
    "typescript": "^4.7.4",
    "unique-names-generator": "^4.7.1",
    "uuid": "^8.3.2",
    "winston": "^3.8.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.5.0",
    "@types/color-thief-node": "^1.0.1",
    "@types/gm": "^1.18.11",
    "@types/js-quantities": "^1.6.3",
    "@types/keyv": "^3.1.4",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-send": "^4.1.3",
    "@types/koa__router": "^8.0.11",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.0.0",
    "@types/node-emoji": "^1.8.1",
    "@types/probe-image-size": "^7.0.1",
    "@types/pug": "^2.0.6",
    "@types/twemoji-parser": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "tsc-watch": "^5.0.3"
  }
}
