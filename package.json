{
  "name": "cubemoji",
  "version": "3.2.0",
  "description": "A Discord emoji bot",
  "main": "Main.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --es-module-specifier-resolution=node build/Main.js",
    "profile": "clinic doctor -- node --es-module-specifier-resolution=node build/Main.js",
    "test": "tsc && export CM_TEST=true && node --es-module-specifier-resolution=node build/tests/Tests.js",
    "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node --inspect --es-module-specifier-resolution=node build/Main.js\"",
    "clean": "rm -rf build/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/cubismod/cubemoji.git"
  },
  "keywords": [
    "discord",
    "emoji",
    "bot"
  ],
  "author": "cubis",
  "bugs": {
    "url": "https://gitlab.com/cubismod/cubemoji/issues"
  },
  "homepage": "https://gitlab.com/cubismod/cubemoji#readme",
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@discordx/pagination": "^1.2.0",
    "@discordx/utilities": "3.4.2",
    "@keyv/sqlite": "^3.0.0",
    "better-sqlite3": "^7.5.0",
    "chokidar": "^3.5.3",
    "dayjs": "^1.10.7",
    "discord.js": "^13.6.0",
    "discordx": "9.3.1",
    "dotenv": "^16.0.0",
    "file-type": "^17.1.1",
    "fuse.js": "^6.5.3",
    "gm": "^1.23.1",
    "got": "^12.0.1",
    "image-downloader": "^4.0.3",
    "js-quantities": "^1.7.6",
    "keyv": "^4.1.1",
    "keyv-file": "^0.2.0",
    "micromatch": "^4.0.4",
    "node-emoji": "^1.11.0",
    "node-object-hash": "^2.3.10",
    "pandemonium": "^2.3.0",
    "probe-image-size": "^7.2.3",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "twemoji-parser": "^13.1.0",
    "unique-names-generator": "^4.7.1",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.4.2",
    "@types/gm": "^1.18.11",
    "@types/js-quantities": "^1.6.3",
    "@types/keyv": "^3.1.3",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^17.0.18",
    "@types/node-emoji": "^1.8.1",
    "@types/probe-image-size": "^7.0.1",
    "@types/pug": "^2.0.6",
    "@types/twemoji-parser": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-uvu": "^0.1.1",
    "tsc-watch": "^4.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3"
  }
}
