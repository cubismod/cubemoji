{
  "name": "cubemoji",
  "author": "cubis",
  "version": "3.5.6",
  "description": "A Discord emoji bot",
  "main": "Main.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --es-module-specifier-resolution=node build/Main.js",
    "inspect": "node --inspect --es-module-specifier-resolution=node build/Main.js",
    "profile": "npx clinic doctor -- node --es-module-specifier-resolution=node build/Main.js",
    "test": "tsc && export CM_TEST=true && node --es-module-specifier-resolution=node build/tests/Tests.js",
    "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"nodenv -- --es-module-specifier-resolution=node --trace-warnings --inspect build/Main.js\"",
    "clean": "rm -r build/",
    "lint": "eslint --ext .ts src/",
    "fix": "eslint --fix --ext .ts src/",
    "audit": "yarn install --audit"
  },
  "eslintPreCommitBaseCommand": "eslint --fix --max-warnings=0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/cubismod/cubemoji.git"
  },
  "keywords": [
    "discord",
    "emoji",
    "bot"
  ],
  "bugs": {
    "url": "https://gitlab.com/cubismod/cubemoji/issues"
  },
  "homepage": "https://gitlab.com/cubismod/cubemoji#readme",
  "dependencies": {
    "@discordx/importer": "^1.2.1",
    "@discordx/koa": "^1.1.2",
    "@discordx/pagination": "^3.4.1",
    "@discordx/utilities": "5.2.1",
    "@keyv/sqlite": "^3.6.5",
    "@koa/router": "^12.0.0",
    "@types/koa": "^2.13.5",
    "aws-sdk": "^2.1338.0",
    "better-sqlite3": "^8.2.0",
    "chokidar": "^3.5.3",
    "dayjs": "^1.11.7",
    "discord.js": "^14.8.0",
    "discordx": "11.7.2",
    "file-type": "^18.2.1",
    "fuse.js": "^6.6.2",
    "gm": "^1.25.0",
    "got": "^12.6.0",
    "image-downloader": "^4.3.0",
    "isbinaryfile": "^5.0.0",
    "js-quantities": "^1.7.6",
    "jstransformer-marked": "^1.3.0",
    "keyv": "^4.5.2",
    "koa": "^2.14.1",
    "koa-body": "^6.0.1",
    "koa-compress": "^5.1.0",
    "koa-html-minifier": "^3.0.0",
    "koa-send": "^5.0.1",
    "micromatch": "^4.0.5",
    "node-emoji": "^1.11.0",
    "node-env-run": "^4.0.2",
    "node-object-hash": "^2.3.10",
    "npm": "^9.6.2",
    "p-queue": "^7.3.4",
    "pandemonium": "^2.4.1",
    "pify": "^6.1.0",
    "probe-image-size": "^7.2.3",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rgb-hex": "^4.0.1",
    "simple-git": "^3.17.0",
    "tslib": "^2.5.0",
    "tsyringe": "^4.7.0",
    "twemoji-parser": "^14.0.0",
    "typescript": "5.0.2",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-discord-webhook": "^2.0.0",
    "winston-gelf-transporter": "^1.0.2",
    "winston-transport": "^4.5.0",
    "yaml": "^2.2.1"
  },
